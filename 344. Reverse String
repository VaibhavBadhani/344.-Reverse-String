class Solution {
    public void reverseString(char[] s) {
        int l=s.length-1;
        char h;
        for(int i=0;i<s.length;i++)
        {
            if(l<=i)
            {
                break;
            }
            h=s[i];
            s[i]=s[l];
            s[l]=h;
            l--;
        }
        
    }
}
